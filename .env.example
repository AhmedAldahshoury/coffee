APP_ENV=local
# Local/dev default uses SQLite to work out-of-the-box without Docker.
# For local Postgres use e.g. postgresql+psycopg://coffee:coffee@localhost:5432/coffee
DATABASE_URL=sqlite:///./coffee.db
JWT_SECRET=change-me
JWT_ALGORITHM=HS256
JWT_EXPIRY_MINUTES=60
DEMO_MODE=false
FAILED_BREW_SCORE=0.0
LOG_LEVEL=INFO
OPTUNA_SKIP_COMPATIBILITY_CHECK=true
